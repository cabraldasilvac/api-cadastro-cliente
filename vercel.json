{
    "version": 2,
    "builds": [{ "source": "api/serverless.ts", "use": "@vercel/node" }],
    "rewrites": [
        {
            "source": "/(.*)",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "destination": "/api/serverless.ts",
            "headers": {
                "Access-Control-Allow-Origin": "https://cadastro-clientes-two.vercel.app/"
            }
        }
    ]
}
