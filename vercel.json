{
    "version": 2,
    "builds": [{ "src": "api/serverless.ts", "use": "@vercel/node" }],
    "routes": [
        {
            "src": "/api/(.*)",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "dest": "/api/serverless.ts",
            "headers": {
                "Access-Control-Allow-Origin": "https://cadastro-clientes-two.vercel.app/"
            }
        }
    ]
}
